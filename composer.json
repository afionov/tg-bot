{
    "name": "afionov/tg-bot",
    "type": "library",
    "description": "Telegram bot library",
    "license": "MIT",
    "keywords": [
        "telegram",
        "bot"
    ],
    "authors": [
        {
            "name": "Alexander Fionov",
            "email": "aa@fionov-dev.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/psr7": "2.4.3"
    },
    "require-dev": {
        "vimeo/psalm": "*",
        "phpunit/phpunit": "9.5.27",
        "guzzlehttp/guzzle": "^7.0"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Bot\\": "src"
        }
    }
}